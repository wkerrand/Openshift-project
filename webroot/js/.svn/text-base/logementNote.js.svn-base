function giveNote(str1,str2,str3){
    $.ajax({
        type: "POST",
        url: "controleurs/notationPage.php",
        data: "note="+str1+"&loId="+str2+"&idm="+str3,
        async: true,
        success: function(msg){
            $('#notation').html(msg);
        }
    });
}

